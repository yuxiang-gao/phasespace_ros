<?xml version="1.0"?>
<launch>
    <arg name="server_ip" />
    <node name="phasespace_ros" pkg="phasespace_ros" type="phasespace_ros_node" output="screen">
        <param name="server_ip" value="$(arg server_ip)"/>
    </node>
    <node pkg="rviz" type="rviz" output="log" name="rviz" args="-d $(find phasespace_ros)/rviz/vis.rviz" />
</launch>